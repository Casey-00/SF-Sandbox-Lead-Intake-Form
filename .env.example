# Salesforce Configuration
# Copy this file to .env and fill in your actual values

# Connected App Credentials
SF_CLIENT_ID=your_client_id_here
SF_CLIENT_SECRET=your_client_secret_here

# Salesforce User Credentials  
SF_USERNAME=your_username_here
SF_PASSWORD=your_password_here
SF_SECURITY_TOKEN=your_security_token_here

# Environment (production or sandbox)
SF_ENVIRONMENT=production
